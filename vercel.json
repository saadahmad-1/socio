{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/uploadthing/(.*)",
      "dest": "/api/uploadthing/$1"
    },
    {
      "src": "/api/webhook/clerk/(.*)",
      "dest": "/api/webhook/clerk/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
